{
  "name": "@h1y/promise-aop",
  "version": "4.1.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": "h1ylabs",
  "keywords": [],
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "yarn clean && tsup",
    "format": "prettier --cache --write \"./src/**/*.{ts,tsx}\"",
    "format:file": "prettier --cache --write",
    "check-types": "bash -c \"tsc --skipLibCheck --noEmit\"",
    "clean": "rimraf ./dist",
    "lint": "eslint --cache --max-warnings=0",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@h1y/loader-config": "workspace:*",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
